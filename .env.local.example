# Local Development Environment Variables
# Copy this file to .env.local and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/jntuh_results_dev

# Redis Configuration (Optional - can use local Redis or skip for development)
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=

# JNTUH Configuration
JNTUH_RESULTS_BASE_URL=http://results.jntuh.ac.in/results/
JNTUH_EXAM_CODE=1323
JNTUH_RESULT_TYPE=grade

# Scraper Configuration (reduced for local testing)
# For production, use full range in .env.local after testing
SCRAPER_WORKER_COUNT=5
# R22 test range - adjust based on actual college codes
HALL_TICKET_START=220000000001
HALL_TICKET_END=220000000100
SCRAPER_DELAY_MS=200
SCRAPER_TIMEOUT_MS=10000
SCRAPER_MAX_RETRIES=3

# Cron Job Configuration
CRON_SECRET=local-dev-secret-123
WATCHER_CRON_SCHEDULE=* * * * *

# API Configuration
API_SECRET_KEY=local-dev-api-key-456
ENABLE_REDIS_CACHE=false
CACHE_TTL_SECONDS=3600

# Environment
NODE_ENV=development
